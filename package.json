{
  "name": "vite-react-ts-electron-template",
  "private": true,
  "version": "0.0.0",
  "main": "main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "yarn build:electron && tsc && vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:3000 && yarn build:electron && cross-env IS_DEV=true electron .",
    "build:electron": "tsc -p electron",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\" ",
    "electron:build.win": "yarn build && electron-builder --win --dir",
    "electron:build.linux": "yarn build && electron-builder --linux appImage",
    "electron:build.test": "yarn build && electron-builder --dir",
    "electron:build.exe": "yarn build && electron-builder --win"
  },
  "build": {
    "appId": "",
    "productName": "project1",
    "copyright": "Copyright Â© 2019 ${author}",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "axios": "^0.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.1",
    "electron-builder": "^22.14.13",
    "less": "^4.1.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "wait-on": "^6.0.1"
  }
}
